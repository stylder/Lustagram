{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
        "react/jsx-filename-extension": 0,
        "no-use-before-define": 0,
        "react/destructuring-assignment": 0,
        "react/prop-types": 0,
        "react/prefer-stateless-function": 0
    },
    "globals": {
        "document": true,
        "foo": true,
        "window": true
    }
}
